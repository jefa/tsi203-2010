<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>UsuarioUI</display-name>
 <context-param>
  <param-name>facelets.BUILD_BEFORE_RESTORE</param-name>
  <param-value>true</param-value>
 </context-param>
 <context-param>
  <param-name>javax.faces.DEFAULT_SUFFIX</param-name>
  <param-value>.xhtml</param-value>
 </context-param>
 <context-param>
  <param-name>javax.faces.STATE_SAVING_METHOD</param-name>
  <param-value>server</param-value>
 </context-param>
 <context-param>
  <param-name>org.ajax4jsf.VIEW_HANDLERS</param-name>
  <param-value>com.sun.facelets.FaceletViewHandler</param-value>
 </context-param>
 <context-param>
  <param-name>org.richfaces.CONTROL_SKINNING</param-name>
  <param-value>disable</param-value>
 </context-param>
 <context-param>
  <param-name>org.richfaces.CONTROL_SKINNING_LEVEL</param-name>
  <param-value>basic</param-value>
 </context-param>
 <filter>
  <display-name>RichFaces Filter</display-name>
  <filter-name>richfaces</filter-name>
  <filter-class>org.ajax4jsf.Filter</filter-class>
  <init-param>
   <param-name>createTempFiles</param-name>
   <param-value>false</param-value>
  </init-param>
 </filter>
 <filter>
  <display-name>Authentication</display-name>
  <filter-name>Authentication</filter-name>
  <filter-class>partuzabook.usuarioUI.Authentication</filter-class>
 </filter>
 <filter-mapping>
  <filter-name>richfaces</filter-name>
  <servlet-name>Faces Servlet</servlet-name>
  <dispatcher>REQUEST</dispatcher>
  <dispatcher>FORWARD</dispatcher>
  <dispatcher>INCLUDE</dispatcher>
 </filter-mapping>
 <filter-mapping>
  <filter-name>Authentication</filter-name>
  <url-pattern>/*</url-pattern>
 </filter-mapping>
 <listener>
  <listener-class>com.sun.faces.config.ConfigureListener</listener-class>
 </listener>
 <servlet>
  <servlet-name>Faces Servlet</servlet-name>
  <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
  <load-on-startup>1</load-on-startup>
 </servlet>
 <servlet>
  <display-name>ContentFeeder</display-name>
  <servlet-name>ContentFeeder</servlet-name>
  <servlet-class>partuzabook.usuarioUI.ContentFeeder</servlet-class>
 </servlet>
 <servlet>
  <display-name>FacebookConnect</display-name>
  <servlet-name>FacebookConnect</servlet-name>
  <servlet-class>partuzabook.usuarioUI.FacebookConnect</servlet-class>
 </servlet>
 <servlet>
  <display-name>GetTagsForContent</display-name>
  <servlet-name>GetTagsForContent</servlet-name>
  <servlet-class>partuzabook.usuarioUI.GetTagsForContent</servlet-class>
 </servlet>
 <servlet>
  <display-name>SendTagsForContent</display-name>
  <servlet-name>SendTagsForContent</servlet-name>
  <servlet-class>partuzabook.usuarioUI.SendTagsForContent</servlet-class>
 </servlet>
 <servlet>
  <display-name>DeleteTagsForContent</display-name>
  <servlet-name>DeleteTagsForContent</servlet-name>
  <servlet-class>partuzabook.usuarioUI.DeleteTagsForContent</servlet-class>
 </servlet>
 <servlet>
  <display-name>UserImageFeeder</display-name>
  <servlet-name>UserImageFeeder</servlet-name>
  <servlet-class>partuzabook.usuarioUI.UserImageFeeder</servlet-class>
 </servlet>
 <servlet>
  <display-name>YoutubeRedirect</display-name>
  <servlet-name>YoutubeRedirect</servlet-name>
  <servlet-class>partuzabook.usuarioUI.YoutubeRedirect</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>MailController</servlet-name>
  <servlet-class>partuzabook.usuarioUI.MailController</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>Faces Servlet</servlet-name>
  <url-pattern>*.jsf</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ContentFeeder</servlet-name>
  <url-pattern>/ContentFeeder</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>GetTagsForContent</servlet-name>
  <url-pattern>/GetTagsForContent</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>SendTagsForContent</servlet-name>
  <url-pattern>/SendTagsForContent</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>DeleteTagsForContent</servlet-name>
  <url-pattern>/DeleteTagsForContent</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>FacebookConnect</servlet-name>
  <url-pattern>/FacebookConnect</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>UserImageFeeder</servlet-name>
  <url-pattern>/UserImageFeeder</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>MailController</servlet-name>
  <url-pattern>/sendmail</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>YoutubeRedirect</servlet-name>
  <url-pattern>/YoutubeRedirect</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>/index.jsp</welcome-file>
 </welcome-file-list>
</web-app>
