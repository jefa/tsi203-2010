<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>EventMB</managed-bean-name>
  <managed-bean-class>partuzabook.superusuarioUI.EventMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>eventoMB</managed-bean-name>
  <managed-bean-class>partuzabook.superusuarioUI.EventoMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginMB</managed-bean-name>
  <managed-bean-class>partuzabook.superusuarioUI.LoginMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>SearchEventMB</managed-bean-name>
  <managed-bean-class>partuzabook.superusuarioUI.SearchEventMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>RegisterAdminMB</managed-bean-name>
  <managed-bean-class>partuzabook.superusuarioUI.RegisterAdminMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>paginaInicioMB</managed-bean-name>
  <managed-bean-class>partuzabook.superusuarioUI.PaginaInicioMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <!-- NavegaciÃ³n -->
	<navigation-rule> 
		<from-view-id>/event.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{EventMB.createEvent}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/paginaInicio.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{EventMB.createEvent}</from-action>
			<from-outcome>failure</from-outcome>
			<to-view-id>/event.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/editEvent.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{EventMB.updateEvent}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/paginaInicio.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{EventMB.updateEvent}</from-action>
			<from-outcome>failure</from-outcome>
			<to-view-id>/editEvent.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/createAdmin.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{RegisterAdminMB.adminRegistration}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/paginaInicio.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{RegisterAdminMB.adminRegistration}</from-action>
			<from-outcome>failure</from-outcome>
			<to-view-id>/createAdmin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>buscarEventos</from-outcome>
			<to-view-id>/busquedaEventos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>crearEvento</from-outcome>
			<to-view-id>/event.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>modEvento</from-outcome>
			<to-view-id>/editEvent.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{loginMB.logout}</from-action>
			<from-outcome>logoutsuccess</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>verEvento</from-outcome>
			<to-view-id>/evento.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{loginMB.validUser}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/paginaInicio.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{loginMB.validUser}</from-action>
			<from-outcome>failure</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/evento.xhtml</from-view-id>
		<navigation-case>
			<from-action>modificarAlbum</from-action>
			<to-view-id>/album.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
	  		<from-outcome>buscar</from-outcome>
	   		<to-view-id>/busquedaEventos.xhtml</to-view-id>
	  	</navigation-case>
	</navigation-rule>	
</faces-config>
