<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>subirFotoMB</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.SubirFotoMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>subirVideoMB</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.SubirVideoMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>paginaInicioMB</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.PaginaInicioMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>eventoMB</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.EventoMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginMB</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.LoginMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>imageLoader</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.ImageLoader</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>SearchEventMB</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.SearchEventMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>PublicProfile</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.PublicProfile</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>notificationsBean</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.NotificationsBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Register</managed-bean-name>
  <managed-bean-class>partuzabook.usuarioUI.Register</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <!-- Navegacion Principal -->
 <navigation-rule>
  <display-name>verEvento</display-name>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>verEvento</from-outcome>
   <to-view-id>/evento.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>verGaleria</display-name>
  <from-view-id>/evento.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>verGaleria</from-outcome>
   <to-view-id>/galeria.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>subirFoto</display-name>
  <from-view-id>/evento.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>subirFoto</from-outcome>
   <to-view-id>/subirFoto.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>subirVideo</display-name>
  <from-view-id>/evento.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>subirVideo</from-outcome>
   <to-view-id>/subirVideo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{loginMB.validUser}</from-action>
   <from-outcome>success</from-outcome>
   <to-view-id>/paginaInicio.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{loginMB.validUser}</from-action>
   <from-outcome>failure</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-action>#{loginMB.logout}</from-action>
   <from-outcome>logoutsuccess</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>verPerfil</display-name>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>verPerfil</from-outcome>
   <to-view-id>/publicProfile.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>verMiPerfil</display-name>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>verMiPerfil</from-outcome>
   <to-view-id>/privateProfile.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/register.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{Register.userRegistration}</from-action>
   <from-outcome>success</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{Register.userRegistration}</from-action>
   <from-outcome>failure</from-outcome>
   <to-view-id>/register.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
